{
  "name": "timestamp",
  "version": "0.0.0",
  "description": "'FreeCodeCamp project: Timestamp Microservice'",
  "main": "index.js",
  "homepage": "https://github.com/vigntom2708/timestamp",
  "bugs": "https://github.com/vigntom2708/timestamp/issues",
  "repository": {
    "url": "https://github.com/vigntom2708/timestamp",
    "type": "git"
  },
  "engine": {
    "node": "6.9.4"
  },
  "author": "Aleksandr I. <vigntom.2708@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "ava": "^0.20.0",
    "supertest": "^3.0.0",
    "xo": "^0.18.2"
  },
  "scripts": {
    "test": "ava",
    "start": "node index.js"
  },
  "ava": {
    "files": [
      "test/**/*.test.js"
    ],
    "source": [
      "index.js",
      "lib/*.js"
    ],
    "concurrency": 5,
    "tap": true
  },
  "xo": {
    "space": 2,
    "semicolon": false,
    "rules": {
      "object-curly-spacing": [
        "error",
        "always"
      ],
      "capitalized-comments": 0
    }
  },
  "dependencies": {
    "moment": "^2.18.1"
  }
}
